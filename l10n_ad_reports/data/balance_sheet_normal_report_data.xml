<?xml version="1.0" encoding="utf-8"?>
<odoo auto_sequence="1">
    <record id="financial_report_balance_sheet_normal" model="account.report">
        <field name="name">Balanç Normal</field>
        <field name="root_report_id" ref="account_reports.balance_sheet"/>
        <field name="filter_date_range" eval="False"/>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="country_id" ref="base.ad"/>
        <field name="filter_multi_company">selector</field>
        <field name="allow_account_audit_status_on_lines" eval="True"/>
        <field name="column_ids">
            <record id="financial_report_balance_sheet_normal_column" model="account.report.column">
                <field name="name">Saldo</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">

            <!-- ACTIU -->
            <record id="balance_ad_line_10000" model="account.report.line">
                <field name="name">TOTAL ACTIU</field>
                <field name="code">l10n_ad_pymes_line_10000</field>
                <field name="aggregation_formula">l10n_ad_pymes_line_11000.balance + l10n_ad_pymes_line_12000.balance</field>
                <field name="horizontal_split_side">left</field>
                <field name="hierarchy_level">0</field>
                <field name="children_ids">

                    <!-- ACTIU NO CORRENT -->
                    <record id="balance_ad_line_11000" model="account.report.line">
                        <field name="name">ACTIU NO CORRENT</field>
                        <field name="code">l10n_ad_pymes_line_11000</field>
                        <field name="aggregation_formula">l10n_ad_pymes_line_11100.balance + l10n_ad_pymes_line_11200.balance + l10n_ad_pymes_line_11300.balance + l10n_ad_pymes_line_11400.balance</field>
                        <field name="children_ids">
                            <!-- IMMOBILITZAT INTANGIBLE -->
                            <record id="balance_ad_line_11100" model="account.report.line">
                                <field name="name">Immobilitzat intangible</field>
                                <field name="code">l10n_ad_pymes_line_11100</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_11110.balance + l10n_ad_pymes_line_11120.balance + l10n_ad_pymes_line_11130.balance + l10n_ad_pymes_line_11140.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_11110" model="account.report.line">
                                        <field name="name">Despeses d'establiment</field>
                                        <field name="code">l10n_ad_pymes_line_11110</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">200</field>
                                    </record>
                                    <record id="balance_ad_line_11120" model="account.report.line">
                                        <field name="name">Despeses d'investigació i desenvolupament</field>
                                        <field name="code">l10n_ad_pymes_line_11120</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">201 + 202</field>
                                    </record>
                                    <record id="balance_ad_line_11130" model="account.report.line">
                                        <field name="name">Fons de comerç</field>
                                        <field name="code">l10n_ad_pymes_line_11130</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">205</field>
                                    </record>
                                    <record id="balance_ad_line_11140" model="account.report.line">
                                        <field name="name">Altres actius intangibles</field>
                                        <field name="code">l10n_ad_pymes_line_11140</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">203 + 204 + 206 + 207 + 208 + 209 + 280 + 290</field>
                                    </record>
                                </field>
                            </record>
                            <!-- IMMOBILITZAT TANGIBLE -->
                            <record id="balance_ad_line_11200" model="account.report.line">
                                <field name="name">Immobilitzat tangible</field>
                                <field name="code">l10n_ad_pymes_line_11200</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_11210.balance + l10n_ad_pymes_line_11220.balance + l10n_ad_pymes_line_11230.balance + l10n_ad_pymes_line_11240.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_11210" model="account.report.line">
                                        <field name="name">Terrenys</field>
                                        <field name="code">l10n_ad_pymes_line_11210</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">210</field>
                                    </record>
                                    <record id="balance_ad_line_11220" model="account.report.line">
                                        <field name="name">Construccions</field>
                                        <field name="code">l10n_ad_pymes_line_11220</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">211</field>
                                    </record>
                                    <record id="balance_ad_line_11230" model="account.report.line">
                                        <field name="name">Instal·lacions tècniques i altres immobilitzats materials</field>
                                        <field name="code">l10n_ad_pymes_line_11230</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">212 + 213 + 214 + 215 + 216 + 217 + 218 + 219 + 281 + 291</field>
                                    </record>
                                    <record id="balance_ad_line_11240" model="account.report.line">
                                        <field name="name">Immobilitzats materials en curs i bestretes</field>
                                        <field name="code">l10n_ad_pymes_line_11240</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">230 + 231 + 232 + 233 + 237 + 238 + 239</field>
                                    </record>
                                </field>
                            </record>
                            <!-- INVERSIONES INMOBILIARIAS -->
                            <record id="balance_ad_line_11300" model="account.report.line">
                                <field name="name">Inversions immobiliàries</field>
                                <field name="code">l10n_ad_pymes_line_11300</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">220 + 221 + 282 + 292</field>
                            </record>
                            <!-- IMMOBILITZAT FINANCER -->
                            <record id="balance_ad_line_11400" model="account.report.line">
                                <field name="name">Immobilitzat financer</field>
                                <field name="code">l10n_ad_pymes_line_11400</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_11410.balance + l10n_ad_pymes_line_11420.balance</field>
                                <field name="children_ids">
                                    <!-- EMPRESES DEL GRUP I ASOCIADES -->
                                    <record id="balance_ad_line_11410" model="account.report.line">
                                        <field name="name">Empreses del grup i associades</field>
                                        <field name="code">l10n_ad_pymes_line_11410</field>
                                        <field name="aggregation_formula">l10n_ad_pymes_line_11411.balance + l10n_ad_pymes_line_11412.balance + l10n_ad_pymes_line_11413.balance</field>
                                        <field name="children_ids">
                                            <record id="balance_ad_line_11411" model="account.report.line">
                                                <field name="name">Instruments de patrimoni</field>
                                                <field name="code">l10n_ad_pymes_line_11411</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">240 + 293</field>
                                            </record>
                                            <record id="balance_ad_line_11412" model="account.report.line">
                                                <field name="name">Crèdits i comptes a cobrar</field>
                                                <field name="code">l10n_ad_pymes_line_11412</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">241 + 242 + 294 + 295</field>
                                            </record>
                                            <record id="balance_ad_line_11413" model="account.report.line">
                                                <field name="name">Altres</field>
                                                <field name="code">l10n_ad_pymes_line_11413</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">999</field> <!-- SI NO POSO ALGO DONA ERROR EL SUMATORI -->
                                            </record>
                                        </field>
                                    </record>
                                    <!-- ALTRES EMPRESES -->
                                    <record id="balance_ad_line_11420" model="account.report.line">
                                        <field name="name">Altres empreses</field>
                                        <field name="code">l10n_ad_pymes_line_11420</field>
                                        <field name="aggregation_formula">l10n_ad_pymes_line_11421.balance + l10n_ad_pymes_line_11422.balance + l10n_ad_pymes_line_11423.balance</field>
                                        <field name="children_ids">
                                            <record id="balance_ad_line_11421" model="account.report.line">
                                                <field name="name">Instruments de patrimoni</field>
                                                <field name="code">l10n_ad_pymes_line_11421</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">250 + 296</field>
                                            </record>
                                            <record id="balance_ad_line_11422" model="account.report.line">
                                                <field name="name">Crèdits i comptes a cobrar</field>
                                                <field name="code">l10n_ad_pymes_line_11422</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">251 + 252 + 253 + 254 + 297 + 298</field>
                                            </record>
                                            <record id="balance_ad_line_11423" model="account.report.line">
                                                <field name="name">Altres</field>
                                                <field name="code">l10n_ad_pymes_line_11423</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">258 + 260 + 265</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                        </field>
                    </record>

                    <!-- ACTIU CORRENT -->
                    <record id="balance_ad_line_12000" model="account.report.line">
                        <field name="name">ACTIU CORRENT</field>
                        <field name="code">l10n_ad_pymes_line_12000</field>
                        <field name="aggregation_formula">l10n_ad_pymes_line_12100.balance + l10n_ad_pymes_line_12200.balance + l10n_ad_pymes_line_12300.balance + l10n_ad_pymes_line_12400.balance + l10n_ad_pymes_line_12500.balance + l10n_ad_pymes_line_12600.balance</field>
                        <field name="children_ids">
                            <!-- ACTIUS NO CORRENTS EN VENDA -->
                            <record id="balance_ad_line_12100" model="account.report.line">
                                <field name="name">Altres actius no corrents en venda</field>
                                <field name="code">l10n_ad_pymes_line_12100</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">580 + 581 + 582 + 583 + 599</field>
                            </record>
                            <!-- EXISTENCIES -->
                            <record id="balance_ad_line_12200" model="account.report.line">
                                <field name="name">Existències</field>
                                <field name="code">l10n_ad_pymes_line_12200</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_12210.balance + l10n_ad_pymes_line_12220.balance + l10n_ad_pymes_line_12230.balance + l10n_ad_pymes_line_12240.balance + l10n_ad_pymes_line_12250.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_12210" model="account.report.line">
                                        <field name="name">Materies primes i consumibles</field>
                                        <field name="code">l10n_ad_pymes_line_12210</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">310 + 311 + 320 + 321 + 322 + 325 + 326 + 327 + 328 + 391 + 392</field>
                                    </record>
                                    <record id="balance_ad_line_12220" model="account.report.line">
                                        <field name="name">Productes en curs de fabricació</field>
                                        <field name="code">l10n_ad_pymes_line_12220</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">330 + 331 + 340 + 341 + 393 + 394</field>
                                    </record>
                                    <record id="balance_ad_line_12230" model="account.report.line">
                                        <field name="name">Productes acabats i mercaderies</field>
                                        <field name="code">l10n_ad_pymes_line_12230</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">300 + 301 + 350 + 351 + 390 + 395</field>
                                    </record>
                                    <record id="balance_ad_line_12240" model="account.report.line">
                                        <field name="name">Bestreta a proveïdors</field>
                                        <field name="code">l10n_ad_pymes_line_12240</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">407</field>
                                    </record>
                                    <record id="balance_ad_line_12250" model="account.report.line">
                                        <field name="name">Altres</field>
                                        <field name="code">l10n_ad_pymes_line_12250</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">360 + 361 + 365 + 366 + 368 + 369 + 396</field>
                                    </record>
                                </field>
                            </record>
                            <!-- CRÈDITS I COMPTES A COBRAR A CURT TERMINI -->
                            <record id="balance_ad_line_12300" model="account.report.line">
                                <field name="name">Crèdits i comptes per cobrar a curt termini</field>
                                <field name="code">l10n_ad_pymes_line_12300</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">43 + 44 + 460 + 470 + 490 + 493 + 472 + 473 + 474</field>
                            </record>
                            <!-- INVERSIONES FINANCERES A CURT TERMINI -->
                            <record id="balance_ad_line_12400" model="account.report.line">
                                <field name="name">Inversions financeres a curt termini</field>
                                <field name="code">l10n_ad_pymes_line_12400</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">53 + 54 + 56 + 593 + 594 + 595 + 597 + 598</field>
                            </record>
                            <!-- TRESORERIA -->
                            <record id="balance_ad_line_12500" model="account.report.line">
                                <field name="name">Tresoreria</field>
                                <field name="code">l10n_ad_pymes_line_12500</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">570 + 571 + 572 + 573 + 574 + 575 + 576</field>
                            </record>
                            <!-- AJUSTAMENTS PER PERIODIFICACIÓ -->
                            <record id="balance_ad_line_12600" model="account.report.line">
                                <field name="name">Ajustaments per periodificació</field>
                                <field name="code">l10n_ad_pymes_line_12600</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">480 + 567</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>

            <!-- PATRIMONI NET I PASSIU -->
            <record id="balance_ad_line_30000" model="account.report.line">
                <field name="name">TOTAL PATRIMONI NET I PASSIU</field>
                <field name="code">l10n_ad_pymes_line_30000</field>
                <field name="aggregation_formula">l10n_ad_pymes_line_20000.balance + l10n_ad_pymes_line_31000.balance + l10n_ad_pymes_line_32000.balance</field>
                <field name="horizontal_split_side">right</field>
                <field name="hierarchy_level">0</field>
                <field name="children_ids">

                    <!-- PATRIMONI NET -->
                    <record id="balance_ad_line_20000" model="account.report.line">
                        <field name="name">PATRIMONI NET</field>
                        <field name="code">l10n_ad_pymes_line_20000</field>
                        <field name="aggregation_formula">l10n_ad_pymes_line_21000.balance + l10n_ad_pymes_line_22000.balance + l10n_ad_pymes_line_23000.balance + l10n_ad_pymes_line_24000.balance + l10n_ad_pymes_line_25000.balance + l10n_ad_pymes_line_26000.balance + l10n_ad_pymes_line_27000.balance</field>
                        <field name="children_ids">
                            <record id="balance_ad_line_21000" model="account.report.line">
                                <field name="name">Capital</field>
                                <field name="code">l10n_ad_pymes_line_21000</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">- 100 - 101 - 102</field>
                            </record>
                            <record id="balance_ad_line_22000" model="account.report.line">
                                <field name="name">Prima d'emissió</field>
                                <field name="code">l10n_ad_pymes_line_22000</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">- 110</field>
                            </record>
                            <record id="balance_ad_line_23000" model="account.report.line">
                                <field name="name">Reserves</field>
                                <field name="code">l10n_ad_pymes_line_23000</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_23100.balance + l10n_ad_pymes_line_23200.balance + l10n_ad_pymes_line_23300.balance + l10n_ad_pymes_line_23400.balance + l10n_ad_pymes_line_23500.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_23100" model="account.report.line">
                                        <field name="name">Reserva Legal</field>
                                        <field name="code">l10n_ad_pymes_line_23100</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 112</field>
                                    </record>
                                    <record id="balance_ad_line_23200" model="account.report.line">
                                        <field name="name">Reserva de revalorització</field>
                                        <field name="code">l10n_ad_pymes_line_23200</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 116</field>
                                    </record>
                                    <record id="balance_ad_line_23300" model="account.report.line">
                                        <field name="name">Reserva per accions propies</field>
                                        <field name="code">l10n_ad_pymes_line_23300</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 115</field>
                                    </record>
                                    <record id="balance_ad_line_23400" model="account.report.line">
                                        <field name="name">Reserves estatuàries</field>
                                        <field name="code">l10n_ad_pymes_line_23400</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 113</field>
                                    </record>
                                    <record id="balance_ad_line_23500" model="account.report.line">
                                        <field name="name">Reserves estatuàries</field>
                                        <field name="code">l10n_ad_pymes_line_23500</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 114 - 118 - 119</field>
                                    </record>
                                </field>
                            </record>
                            <record id="balance_ad_line_24000" model="account.report.line">
                                <field name="name">Instruments de capital propis</field>
                                <field name="code">l10n_ad_pymes_line_24000</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">- 109 - 108</field>
                            </record>
                            <record id="balance_ad_line_25000" model="account.report.line">
                                <field name="name">Resultats d'exercicis anteriors</field>
                                <field name="code">l10n_ad_pymes_line_25000</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_25100.balance + l10n_ad_pymes_line_25200.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_25100" model="account.report.line">
                                        <field name="name">Romanent</field>
                                        <field name="code">l10n_ad_pymes_line_25100</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 120</field>
                                    </record>
                                    <record id="balance_ad_line_25200" model="account.report.line">
                                        <field name="name">Resultats negatius d'exercicis anteriors</field>
                                        <field name="code">l10n_ad_pymes_line_25200</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 121</field>
                                    </record>
                                </field>
                            </record>
                            <record id="balance_ad_line_26000" model="account.report.line">
                                <field name="name">Resultat de l'exercici</field>
                                <field name="code">l10n_ad_pymes_line_26000</field>
                                <field name="action_id" ref="account_reports.action_account_report_pl"/>
                                <field name="expression_ids">
                                    <record id="balance_ad_line_26000_balance" model="account.report.expression">
                                        <field name="label">balance</field>
                                        <field name="engine">aggregation</field>
                                        <field name="subformula">cross_report(l10n_ad_reports.financial_report_es_profit_and_loss)</field>
                                        <field name="formula">ad_pl_year_sum.balance</field>
                                        <field name="date_scope">from_fiscalyear</field>
                                    </record>
                                </field>
                            </record>
                            <record id="balance_ad_line_27000" model="account.report.line">
                                <field name="name">Dividend a compte</field>
                                <field name="code">l10n_ad_pymes_line_27000</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">- 557</field>
                            </record>
                        </field>
                    </record>

                    <!-- PASSIU NO CORRENT -->
                    <record id="balance_ad_line_31000" model="account.report.line">
                        <field name="name">PASSIU NO CORRENT</field>
                        <field name="code">l10n_ad_pymes_line_31000</field>
                        <field name="aggregation_formula">l10n_ad_pymes_line_31100.balance + l10n_ad_pymes_line_31200.balance + l10n_ad_pymes_line_31300.balance + l10n_ad_pymes_line_31400.balance</field>
                        <field name="children_ids">
                            <record id="balance_ad_line_31100" model="account.report.line">
                                <field name="name">Provisions a llarg termini</field>
                                <field name="code">l10n_ad_pymes_line_31100</field>
                                <field name="foldable" eval="True"/>
                                <field name="account_codes_formula">-141 - 143 - 145 - 146</field>
                            </record>
                            <record id="balance_ad_line_31200" model="account.report.line">
                                <field name="name">Deutes a llarg termini</field>
                                <field name="code">l10n_ad_pymes_line_31200</field>
                                <field name="aggregation_formula">l10n_ad_pymes_line_31210.balance + l10n_ad_pymes_line_31220.balance + l10n_ad_pymes_line_31230.balance</field>
                                <field name="children_ids">
                                    <record id="balance_ad_line_31210" model="account.report.line">
                                        <field name="name">Obligacions i altres valora negociables</field>
                                        <field name="code">l10n_ad_pymes_line_31210</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 177 - 179</field>
                                    </record>
                                    <record id="balance_ad_line_31220" model="account.report.line">
                                        <field name="name">Deutes amb entitats de crèdit</field>
                                        <field name="code">l10n_ad_pymes_line_31220</field>
                                        <field name="foldable" eval="True"/>
                                        <field name="account_codes_formula">- 170 - 174</field>
                                    </record>
                                    <record id="balance_ad_line_31230" model="account.report.line">
                                        <field name="name">Altres</field>
