            <!-- RESULTAT DE L'EXERCICI -->
            <record id="ad_resultat_exercici_n" model="account.report.line">
                <field name="name">RESULTAT DE L'EXERCICI</field>
                <field name="hierarchy_level">0</field>
                <field name="code">ad_resultat_exercici_n</field>
                <field name="expression_ids">
                    <record id="ad_resultat_exercici_n_balance" model="account.report.expression">
