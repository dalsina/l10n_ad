                        <field name="engine">aggregation</field>
                        <field name="formula">ad_resultat_operacions_n.balance + ad_pl_n_27.balance + ad_pl_n_28.balance</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="ad_pl_n_27" model="account.report.line">
                        <field name="name">Altres ingresos no recurrents</field>
                        <field name="code">ad_pl_n_27</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="ad_pl_n_27_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">account_codes</field>
                                <field name="formula">- 778</field>
                            </record>
                        </field>
                    </record>
                    <record id="ad_pl_n_28" model="account.report.line">
                        <field name="name">Altres despeses no recurrents</field>
                        <field name="code">ad_pl_n_28</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="ad_pl_n_28_balance" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">account_codes</field>
                                <field name="formula">- 678</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>

